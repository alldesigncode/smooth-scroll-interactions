      fig.appendChild(img);
      document.querySelector("section.images").appendChild(fig);
    });
    document.querySelector("section.images").appendChild(iphone);
  };
  const initScroll = () => {
    const verticalScrollbar = _smoothScrollbarDefault.default.init(scrollbar, {
      damping: 0.1,
